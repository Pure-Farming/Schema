{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "/project/research-project",
    "description": "Record of research project details including title, organisation, dates, principal researcher, summary, tags.",
    "type": "object",
    "allOf": [
        {
            "$ref": "https://raw.githubusercontent.com/Pure-Farming/DataLinker-Geospatial/master/types/ResourceType.json"
        }
    ],
    "properties": {
        "projectCode": {
            "$ref": "https://raw.githubusercontent.com/Pure-Farming/DataLinker-Geospatial/master/types/IdentifierType.json",
            "description": "Project code or number for the project, in the form of a scheme and id."
        },
        "leadOrganization": {
            "$ref": "../types/OrganizationType.json",
            "description": "The lead organisation of the project record. May be the RDC and the holding."
        },
        "researchOrganizations": {
            "type": "array",
            "items": {
                "$ref": "../types/OrganizationType.json"
            },
            "description": "Organisations participating in the project."
        },
        "principalResearcher": {
            "type": "string",
            "description": "The principal or lead researcher."
        },
        "projectStartDate": {
            "$ref": "https://raw.githubusercontent.com/Pure-Farming/DataLinker-Geospatial/master/types/DateTimeType.json",
            "description": "Date the project started."
        },
        "projectEndDate": {
            "$ref": "https://raw.githubusercontent.com/Pure-Farming/DataLinker-Geospatial/master/types/DateTimeType.json",
            "description": "Date the project ended."
        },
        "readiness": {
            "type": "string",
            "description": "Technology Readiness Level or range."
        },
        "abstract": {
            "type": "string",
            "description": "Abstract of project."
        },
        "projectContent": {
            "$ref": "../types/ProjectContentType.json",
            "description": "Statements of background, need, objectives and body of the project."
        },
        "contactEmail": {
            "type": "string",
            "description": "Email address of the contact for the project."
        },
        "commercialOpportunities": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "Commercial opportunities for the project."
        },
        "industries": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "Industries relevant to the project."
        },
        "focusAreas": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "Project focus areas."
        },
        "technologyAreas": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "Technology areas relevant to the project."
        },
        "sustainabilities": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "Sustainabilities addressed by the project."
        },
        "publishedDate": {
            "$ref": "https://raw.githubusercontent.com/Pure-Farming/DataLinker-Geospatial/master/types/DateTimeType.json",
            "description": "Date the project was published."
        },
        "locations": {
            "type": "array",
            "items": {
                "$ref": "https://raw.githubusercontent.com/Pure-Farming/DataLinker-Geospatial/master/types/PostalAddress.json"
            },
            "description": "Locations where the project was conducted.  Not all fields may be relevant."
        },
        "generatedTags": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "Tags auto-generated for the project."
        },
        "configuredTags": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "Tags configured or manually entered for the project."
        },
        "generatedAbstract": {
            "type": "string",
            "description": "Auto-generated abstract of project."
        }
    },
