{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "/types/landUseResource",
    "type": "object",
    "allOf": [
        {
            "$ref": "./spatialResource.json"
        }
    ],
    "properties": {
        "activities":{
            "type":"array",
            "items":{
                "$ref": "https://raw.githubusercontent.com/Datalinker-Org/Geospatial/master/types/LandUseActivityType.json"
            }
        },
        "classifications":{
            "type":"array",
            "items":{
                "$ref": "https://raw.githubusercontent.com/Datalinker-Org/Geospatial/master/types/ClassificationType.json"
            }
        }
    }
}