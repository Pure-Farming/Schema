{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "/types/icarStockArrivalReasonType",
    "description": "Defines the number of stock that arrived for a specified arrival reason.",
    "type": "object",
    "required": [
        "reason"
    ],
    "properties": {
        "reason": {
            "$ref": "https://raw.githubusercontent.com/Pure-Farming/ICAR/ADE-1/enums/icarArrivalReasonType.json",
            "description": "The reason for the arrival of the stock"
        },
        "count": {
            "type": "integer",
            "description": "The number of stock that arrived for the specified reason"
        }
    },
    "examples": [
        {
            "arrivals": {
                "reason": "Purchase",
                "count": 85
            }
        }
    ]
}